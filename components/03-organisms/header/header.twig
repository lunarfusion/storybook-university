{%
  set classes = [
    'header',
    'drk-background',
    'region',
    'region--' ~ region|clean_class,
  ]
%}

<div{{ attributes.addClass(classes) }}>
  {% block branding %}{{ branding }}{% endblock %}
  {% block button %}{{ button }}{% endblock %}
  {% block menu %}{{ menu }}{% endblock %}
</div>
